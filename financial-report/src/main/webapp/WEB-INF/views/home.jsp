<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
	<title>智慧免疫云-财务报表专用平台</title>
	<link href="resources/css/style.css" rel="stylesheet" type="text/css">
</head>
<body>
	<div class="header">
		<div class="logo"></div>
	</div>
	<div class="index_box">
		<div class="index_pic"></div>
		<div class="login_box">
			<div class="login_panel">
				<div class="login_form">
					<div class="login_form_title">登录</div>
					<div class="login_text_u">
						<div class="icon"></div>
						<input type="text" name="username" placeholder="用户名">
					</div>
					<div class="login_text_p">
						<div class="icon"></div>
						<input type="password" name="userpwd" placeholder="密码">
					</div>
					<div class="login_code">
						<input type="text" name="usercode" placeholder="验证码">
						
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
